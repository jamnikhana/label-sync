{
  "strict": false,
  "labels": {
    "FAQ": "blue",
    "kind/bug": {
      "color": "yellow",
      "description": "All issues which are bugs."
    }
  },
  "repositories": [
    "prisma/*",
    {
      "paths": "prisma/one-*",
      "labels": {
        "Feature: Prisma One": "green",
        "field/express": {
          "color": "red",
          "description": "Issues related to ExpressJS."
        }
      },
      "strict": true
    },
    {
      "paths": "prisma/two-*",
      "labels": {
        "Feature: Prisma Two": "green",
        "field/express": {
          "color": "red",
          "description": "Issues related to ExpressJS."
        }
      }
    },
    {
      "paths": "prisma/three-*"
    }
  ],
  "publish": {
    "branch": "master"
  }
}
